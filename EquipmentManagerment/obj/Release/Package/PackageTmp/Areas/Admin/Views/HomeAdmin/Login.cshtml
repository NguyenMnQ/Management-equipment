@model EquipmentManagerment.Context.Account

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login V4</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/Areas/Assets/AssetsLogin/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetssLogin/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Areas/Assets/AssetsLogin/css/main.css">
    <!--===============================================================================================-->
</head>
<body>
    @using (Html.BeginForm("Login", "HomeAdmin", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        <div class="limiter">
            <div class="container-login100" style="background-image: url('images/bg-01.jpg');">
                <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                    <form class="login100-form validate-form">
                        <span class="login100-form-title p-b-49">
                            login
                        </span>
                        <div class="form-group wrap-input100 validate-input m-b-23">
                            @Html.LabelFor(m => m.TenDangNhap)
                            <div class="col-md-10 ">
                                @*@Html.TextBoxFor(m => m.TenDangNhap, "", new { @class = "input100", })*@
                                <input class="input100" type="text" name="TenDangNhap" placeholder="">
                                <span class="focus-input100" data-symbol="&#xf206;"></span>
                            </div>
                        </div>
                        
                        <div class="form-group wrap-input100 validate-input">
                            @Html.LabelFor(m => m.MatKhau)
                            <div>
                                @Html.PasswordFor(m => m.MatKhau, new { @class = "input100" })
                                <span class="focus-input100" data-symbol="&#xf190;"></span>
                            </div>
                        </div>
                        <div class="text-right p-t-8 p-b-31">
                            <a href="#">
                                forgot password?
                            </a>
                        </div>
                        <div style="padding-left: 40px;">
                            <div class="g-recaptcha" data-sitekey="6lcqp58jaaaaaednkuey2ogicoskcyujmnb5t_co"></div>
                        </div>
                        @ViewBag.Message
                        <div class="container-login100-form-btn">
                            <div class="wrap-login100-form-btn">
                                <div class="login100-form-bgbtn"></div>
                                <button class="login100-form-btn" type="submit" value="login">
                                    Login
                                </button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    }

    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="~/Areas/Assets/AssetsLogin/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Assets/AssetsLogin/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Assets/AssetsLogin/vendor/bootstrap/js/popper.js"></script>
    <script src="~/Areas/Assets/AssetsLogin/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Assets/AssetsLogin/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Assets/AssetsLogin/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/Areas/Assets/AssetsLogin/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Assets/AssetsLogin/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/Areas/Assets/AssetsLogin/js/main.js"></script>

    <script src="http://www.google.com/recaptcha/api.js" type="text/javascript" async defer></script>

</body>
</html>
